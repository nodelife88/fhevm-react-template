{
  "compilerOptions": {
    "target": "ES2022",
    "module": "CommonJS",
    "moduleResolution": "node",
    "lib": ["ES2022"],
    "types": ["node"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "paths": {
      "@fhevm-sdk": ["./node_modules/fhevm-sdk"]
    }
  },
  "include": [
    "packages/**/*",
    "scripts/**/*"
  ]
}
