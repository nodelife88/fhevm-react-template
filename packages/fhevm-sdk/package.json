{
  "name": "fhevm-sdk",
  "version": "0.1.1",
  "description": "SDK for FHEVM (Fully Homomorphic Encryption Virtual Machine) integration",
  "keywords": ["fhevm", "fhe", "encryption", "blockchain", "ethereum"],
  "author": "Your Name <truongquoctuan61@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/HadesGuard/fhevm-sdk.git",
    "directory": "fhevm-sdk"
  },
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "default": "./dist/core/index.js"
    },
    "./storage": {
      "types": "./dist/storage/index.d.ts",
      "default": "./dist/storage/index.js"
    },
    "./types": {
      "types": "./dist/fhevmTypes.d.ts",
      "default": "./dist/fhevmTypes.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "default": "./dist/react/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "watch": "tsc -p tsconfig.json --watch",
    "clean": "rm -rf dist",
    "test": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "dependencies": {
    "idb": "^8.0.3"
  },
  "peerDependencies": {
    "@fhevm/mock-utils": "^0.1.0",
    "@zama-fhe/relayer-sdk": "^0.2.0",
    "ethers": "^6.13.4",
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/node": "~18.19.50",
    "@types/react": "~19.0.7",
    "@vitest/coverage-v8": "2.1.9",
    "fake-indexeddb": "~6.0.0",
    "jsdom": "^27.0.0",
    "typescript": "~5.8.2",
    "vitest": "~2.1.8"
  }
}
